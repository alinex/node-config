  "main": "./dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.js --exec 'yarn lint && yarn unit'",
    "build": "babel src -d dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "precommit": "npm run lint",
